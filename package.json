{
  "name": "vitesse-nuxt-electron",
  "version": "0.0.1",
  "private": true,
  "packageManager": "pnpm@8.7.1",
  "description": "Vitesse + Nuxt + Electron starter template",
  "author": {
    "name": "YunYouJun",
    "email": "me@yunyoujun.cn",
    "url": "https://yunyoujun.cn"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/YunLeFun/vitesse-nuxt-electron"
  },
  "main": "dist-electron/main.js",
  "scripts": {
    "build": "npm run build:electron",
    "build:nuxt": "nuxi build",
    "build:electron": "nuxi build --prerender && electron-builder",
    "dev": "nuxi dev",
    "dev:pwa": "VITE_PLUGIN_PWA=true nuxi dev",
    "start": "node .output/server/index.mjs",
    "typecheck": "vue-tsc --noEmit",
    "lint": "eslint .",
    "postinstall": "nuxi prepare",
    "generate": "nuxi generate",
    "start:generate": "npx serve .output/public"
  },
  "devDependencies": {
    "@antfu/eslint-config": "^0.41.0",
    "@iconify-json/carbon": "^1.1.21",
    "@iconify-json/twemoji": "^1.1.12",
    "@nuxt/devtools": "^0.8.2",
    "@nuxtjs/color-mode": "^3.3.0",
    "@pinia/nuxt": "^0.4.11",
    "@unocss/eslint-config": "^0.55.7",
    "@unocss/nuxt": "^0.55.7",
    "@vite-pwa/nuxt": "^0.1.0",
    "@vueuse/nuxt": "^10.4.1",
    "consola": "^3.2.3",
    "electron": "^26.1.0",
    "electron-builder": "^24.6.4",
    "eslint": "^8.48.0",
    "nuxt": "^3.7.0",
    "nuxt-electron": "^0.6.0",
    "pinia": "^2.1.6",
    "typescript": "^5.2.2",
    "vite-plugin-electron": "0.14.0",
    "vite-plugin-electron-renderer": "^0.14.5",
    "vue-tsc": "^1.8.8"
  }
}
